<?xml version='1.0' encoding='utf-8'?>
<widget id="com.nevaforget.weatherapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <name>Weatherapp</name>
    <description>
        Check the weather.
    </description>
    <author email="info@nevaforget.de" href="http://nevaforget.de">
        Dominik Kressler
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon density="ldpi" platform="android" src="www/images/res/drawable-ldpi/ic_launcher.png" />
    <icon density="mdpi" platform="android" src="www/images/res/drawable-mdpi/ic_launcher.png" />
    <icon density="hdpi" platform="android" src="www/images/res/drawable-hdpi/ic_launcher.png" />
    <icon density="xhdpi" platform="android" src="www/images/res/drawable-xhdpi/ic_launcher.png" />
</widget>
